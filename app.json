{
  "expo": {
    "name": "Mama Recipe",
    "slug": "mama-recipe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/mama-recipe-logo.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "scheme": "acme",
    "web": { "bundler": "metro" }
  }
}
